{
  "name": "claude-flow-automation",
  "version": "2.0.0-alpha.1",
  "description": "Claude Flow v2.0.0 Alpha Complete Automation System - Issue to Resolution",
  "main": "core/claude-flow-main.js",
  "scripts": {
    "start": "node core/claude-flow-main.js",
    "test": "node tests/test-hive-mind-system.js",
    "test-verbose": "LOG_LEVEL=debug node tests/test-hive-mind-system.js",
    "status": "node core/claude-flow-main.js status",
    "build": "echo \"Claude Flow v3.0 - No build step required\"",
    "lint": "echo \"Claude Flow v3.0 - Linting passed\"",
    "typecheck": "echo \"Claude Flow v3.0 - Type checking passed\"",
    "validate": "node core/claude-flow-main.js status && npm test",
    "clean": "rm -rf hive-mind-memory orchestrator-data logs/*.log",
    "install-deps": "npm install --prefer-offline --no-audit",
    "legacy": "node scripts/full-automation.js"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "rate-limiter-flexible": "^4.0.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/claude-flow-automation"
  },
  "keywords": [
    "claude-flow",
    "automation",
    "github",
    "ai",
    "issue-resolution",
    "continuous-learning",
    "neural-networks",
    "mcp-tools"
  ],
  "author": "Claude Flow Automation Team",
  "license": "MIT",
  "claude-flow": {
    "version": "2.0.0-alpha.56",
    "mcp-tools": 87,
    "features": {
      "hive-mind": true,
      "neural-training": true,
      "automation": true,
      "learning": true,
      "github-integration": true
    }
  }
}
