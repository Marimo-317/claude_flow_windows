    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "fs-extra": "^11.2.0",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1"
